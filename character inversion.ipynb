   "source": [
    "def reverse_alternate(str1):\n",
    "  result = \"\" \n",
    "  for i in range(len(str1)):\n",
    "    if i % 2 == 0:\n",
    "      result += str1[i]\n",
    "    else:\n",
    "      result += str1[i][::-1]\n",
    "  return result\n",
    "\n",
    "str1 = \"The sky is blue\"\n",
    "print(reverse_alternate(str1))\n",
    "\n"
   ]
